{
  "name": "next-codegen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "development": "next dev",
    "check-types": "tsc --pretty --noEmit",
    "build": "next build",
    "start-server": "next start",
    "dev": "concurrently --kill-others \"npm run development\"",
    "start": "concurrently --kill-others \"npm run start-server\"",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.tsx --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,md}": "prettier --write --ignore-unknown",
    "src/**/*.{ts,tsx}": "yarn run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browser": {
    "fs": false,
    "child_process": false
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/material": "^5.14.17",
    "@react-oauth/google": "^0.12.1",
    "@types/react-image-magnify": "^2.7.4",
    "axios": "^1.0.0",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.2.2",
    "currency-symbol-map": "^5.1.0",
    "date-fns": "^3.3.1",
    "formik": "^2.4.5",
    "i18next": "^22.4.15",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "next": "13.3.4",
    "next-i18next": "^13.2.2",
    "next-pwa": "^5.6.0",
    "nextjs-progressbar": "^0.0.16",
    "notistack": "^2.0.5",
    "plaiceholder": "^3.0.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.9.2",
    "react-cookie-consent": "^9.0.0",
    "react-cookienotice": "^6.0.3",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-facebook-login": "^4.1.1",
    "react-hook-form": "^7.49.2",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^12.2.2",
    "react-image-magnify": "^2.7.4",
    "react-js-pagination": "^3.0.3",
    "react-modal": "^3.16.1",
    "react-otp-input": "^3.1.1",
    "react-player": "^2.16.0",
    "react-rating-stars-component": "^2.2.0",
    "react-razorpay": "^2.0.1",
    "react-scroll": "^1.9.0",
    "react-share": "^5.0.3",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "sharp": "^0.32.6",
    "styled-components": "^6.1.1",
    "swr": "^2.2.4",
    "yup": "^1.3.3",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "18.6.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-modal": "^3.16.3",
    "@types/react-slick": "^0.23.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "css-loader": "^6.8.1",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^7.0.0",
    "prettier": "3.0.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.3.3"
  }
}
